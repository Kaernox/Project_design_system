{
    "name": "akk-datepicker",
    "scope": [
        {
            "attr": "label",
            "bind": "@",
            "value": "My DatePicker"
        },
        {
            "attr": "req",
            "bind": "@",
            "value": ""
        },
        {
            "attr": "model",
            "bind": "=",
            "value": "view.date"
        },
        {
            "attr": "event",
            "bind": "@",
            "value": ""
        }
        
    ],
    "descritpion": "The akk-datepicker allows you to pick a date and store it as a javascript date object in the value provided in the model property."
}
